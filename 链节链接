class LinkNote:#火车车厢与链节
    def __init__(self,value=0,next=None):
        self.value=value
        self.next=next

class LinkList:
    def __init__(self):
        self.head=None
        
    def append(self,value):
        if not self.head:
            self.head=LinkNote(value)
        else:
            current=self.head
            while current.next:
               current=current.next
            current.next=LinkNote(value)
            

#使用
ll=LinkList()
ll.append(1)
ll.append(2)
